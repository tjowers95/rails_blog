<body>
  <div id="container">
  <div id="content">
    <%=yield %>
    <hr>
    <h6>
      <% if logged_in? %>
        <%= "Signed in as #{current_user.email}" %>
        <%= link_to "(Sign Out)", logout_path %>
      <% else %>
        <%= link_to "(Sign In)", login_path %>
      <% end %>
     </h6>
   </div>
 </div>
</body>
